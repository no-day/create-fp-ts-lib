{
  "name": "create-fp-ts-lib",
  "homepage": "https://github.com/no-day/create-fp-ts-lib",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "peerDependencies": {
    "fp-ts": "^2.9.5"
  },
  "bin": {
    "create-fp-ts-lib": "bin/index.js"
  },
  "dependencies": {
    "@types/mustache": "^4.1.1",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "fast-check": "^2.13.0",
    "fp-ts": "^2.10.5",
    "jest": "^26.6.3",
    "markdown-magic": "^2.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-jsdoc": "^0.3.13",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "build": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "prepublish": "yarn build",
    "md": "yarn markdown --config markdown.config.js",
    "md:watch": "while inotifywait -e close_write README.md; do yarn md; done",
    "docs-preview:watch": "cd docs; bundle exec jekyll serve --livereload",
    "docs-preview": "cd docs; bundle exec jekyll serve"
  }
}
